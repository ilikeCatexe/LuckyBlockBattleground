local Section = Tab:AddSection({
	Name = "Mod"
})

function esp(item)
    if not item:FindFirstChild("Highlight") then
        local espitem = Instance.new("Highlight")
        espitem.Parent = item
    else
        item.Highlight:Destroy()
        local espitem = Instance.new("Highlight")
        espitem.Parent = item
    end
end

Tab:AddButton({
	Name = "Become WeaponMaster Boss",
	Callback = function()
        _G.ClearitemCheck = false
        game.Players.LocalPlayer.Character.Humanoid.HealthChanged:Connect(function()
            if script.Parent.Health <= 0 then
                if _G.ClearitemCheck then return end
                _G.ClearitemCheck = true
                for i,v in pairs(game.Workspace:GetChildren()) do
                    if v:IsA("Tool") then
                        v:Destroy()
                    end
                end
            end
        end)
        --[[for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do
            if v:IsA("Tool") then
                v:Destroy()
            end
        end--]]
        task.wait(1)
        for i = 1,60 do
            game.ReplicatedStorage.SpawnGalaxyBlock:FireServer()
        end
        task.wait(3)
        for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren())  do
            if v:IsA("Tool") then
                v.Parent = game.Players.LocalPlayer.Character
            end
        end
        task.wait(1)
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
        	if v:IsA("Tool") then
        		v.Parent = workspace
        	end
        end
        task.wait(1)
        for i = 1,20 do
            game.ReplicatedStorage.SpawnRainbowBlock:FireServer()
        end
  	end    
})

Tab:AddButton({
	Name = "Clear WeaponDrop",
	Callback = function()
        for i,v in pairs(game.Workspace:GetChildren()) do
            if v:IsA("Tool") then
                v:Destroy()
            end
        end
  	end   
})

Tab:AddParagraph("How To Use","To using WeaponMaster Boss make sure you don't have any items in your backpack and when you died use clear WeaponDrop")

Tab:AddToggle({
	Name = "Esp Players",
	Default = false,
	Callback = function(Value)
		_G.espplayer = Value
	end    
})


spawn(function()
    while task.wait() do
        pcall(function()
            if _G.autohitp then
                local owntool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
                onHit(game.Players[splr].Character,owntool.Handle)
            end
            if _G.autohitall then
                local owntool = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool")
                for i,v in pairs(game.Players:GetChildren()) do
                    onHit(v.Character,owntool.Handle)
                end
            end
            if _G.espplayer then
                for i,v in pairs(game.Players:GetChildren()) do
                    if not v:FindFirstChild("FancyHat") then
                        esp(v.Character)
                    end
                end
            elseif _G.espplayer == false then
                for i,v in pairs(game.Workspace:GetChildren()) do
                    if v:IsA("Model") and v:FindFirstChild("Highlight") then
                        v.Highlight:Destroy()
                    end
                end
            end
        end)
    end
end)

OrionLib:Init()
